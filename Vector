#include<bits/stdc++.h>
using namespace std;

int main()
{
    #ifdef TarekHasan
        freopen("input.txt", "r", stdin);
    #endif // TarekHasan

    int n;
    cin >> n; /// scanf("%d",&n);

    vector< int > v;
    for(int i=0; i<n; i++)
    {
        int temp;
        cin >> temp;
        v.push_back(temp);
    }


    // lambda
    auto print = [&]()
    {
        for(int i=0; i<n; i++)
            printf("%d ", v[i]);
        printf("\n");
    };

    print();

    /**
    print();

    /// sort elements in ascending order
    sort(v.begin(), v.end(), less<int>());
    reverse(v.begin(), v.end());

    print();
    **/

    /**

    /// sort elements in descending order
    sort(v.begin(), v.end(), greater<int>());
    print();

    **/


    return 0;
}
