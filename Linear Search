#include<stdio.h>
int linearSearch(int arr[], int n, int key)
{
    for(int i=0; i<n; i++)
    {
        if(arr[i]==key)
            return i;
    }


    return -1;
}

int main()
{
    int arr[9] = {3, 6, 9, 2, 5, 8, 1, 4, 7};
    int n = 9;
    printf("%d\n",linearSearch(arr, n, 7));
    return 0;
}
