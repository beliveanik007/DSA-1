#include<iostream>
#include<queue>
using namespace std;
int main()
{
    queue< int > q;

    q.push(5);
    q.push(7);
    q.push(11);
    q.push(13);


    cout << "Length of the Queue: " << q.size() << endl;

    cout << "Front element of the Queue: " << q.front() << endl;
    q.pop();

    cout << "Front element of the Queue: " << q.front() << endl;


    while(!q.empty())
    {
        q.pop();
    }


    if(q.empty())
        cout << "Queue is Empty.\n";
    else
        cout << "Queue is not Empty.\n";

    return 0;
}
